  "name": "@ntegral/nestjs-opentok",
  "version": "0.0.1",
  "description": "Provides an injectable opentok client to nestjs modules",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf lib",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public"
  },
  "keywords": [
    "nestjs",
    "opentok",
    "tokbox"
  ],
  "author": "Dexter Hardy",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "@nestjs/testing": "^6.5.3",
    "@types/opentok": "^2.3.5",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "opentok": "^2.9.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.5.3",
    "@nestjs/core": "^6.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
